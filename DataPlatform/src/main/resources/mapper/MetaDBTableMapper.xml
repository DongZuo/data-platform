<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxb.cms.dao.MetaDB">
    <select id="selectByTemplateId" resultType="">
      select a.db_eng_name, b.tb_eng_name
      from meta_db as a
      join (meta_table as b
      on a.db_id == b.db_id
    </select>
</mapper>